{
  "name": "angular-lib",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "angular-lib/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{options.outputPath}"],
      "options": {
        "project": "angular-lib/ng-package.json",
        "outputPath": "build/angular-lib"
      },
      "configurations": {
        "production": {
          "tsConfig": "angular-lib/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "angular-lib/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "angular-lib/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 6007,
        "configDir": "angular-lib/.storybook"
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "build/storybook/angular",
        "configDir": "angular-lib/.storybook"
      }
    }
  }
}
